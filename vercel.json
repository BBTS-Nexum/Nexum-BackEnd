{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "AZURE_SQL_DRIVER": "@azure-sql-driver",
    "AZURE_SQL_SERVER": "@azure-sql-server",
    "AZURE_SQL_PORT": "@azure-sql-port",
    "AZURE_SQL_DATABASE": "@azure-sql-database",
    "AZURE_SQL_USERNAME": "@azure-sql-username",
    "AZURE_SQL_PASSWORD": "@azure-sql-password",
    "AZURE_SQL_ENCRYPT": "@azure-sql-encrypt",
    "AZURE_SQL_TRUST_SERVER_CERTIFICATE": "@azure-sql-trust-server-certificate",
    "AZURE_SQL_CONNECTION_TIMEOUT": "@azure-sql-connection-timeout"
  }
}
